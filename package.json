{
  "name": "freezeobject",
  "version": "1.0.0",
  "description": "make a safe freeze object freezeobject - better usage [modified node internals] than as defined and used in node internals",
  "main": "index.js",
  "exports": {
    "require": "./index.js",
    "import": "./index.mjs"
  },
  "unpkg": "index.min.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ganeshkbhat/freezeobject.git"
  },
  "bugs": {
    "url": "https://github.com/ganeshkbhat/freezeobject/issues"
  },
  "homepage": "https://github.com/ganeshkbhat/freezeobject#readme",
  "scripts": {
    "test": "mocha --reporter spec --recursive --timeout 60000",
    "uglify": "uglifyjs --compress --mangle reserved=['$','require','exports'] index.js -o index.min.js",
    "gitadd": "git add .",
    "tsc": "tsc",
    "build": "webpack"
  },
  "pre-commit": {
    "run": [
      "uglify",
      "gitadd",
      "test"
    ]
  },
  "keywords": [],
  "author": "Ganesh <ganeshsurfs@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/eslint": "^8.56.2",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.16",
    "chai": "^4.4.1",
    "live-server": "^1.2.2",
    "mocha": "^10.2.0",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.4",
    "sinon": "^17.0.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "uglify-js": "^3.17.4",
    "unimported": "^1.31.1",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  }
}
